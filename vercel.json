{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/addon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/realdebrid=(?<token>[^/]+)/(?<path>.*)",
      "dest": "/api/addon.js?token=$token&path=$path"
    },
    {
      "src": "/alldebrid=(?<token>[^/]+)/(?<path>.*)",
      "dest": "/api/addon.js?token=$token&path=$path"
    },
    {
      "src": "/torbox=(?<token>[^/]+)/(?<path>.*)",
      "dest": "/api/addon.js?token=$token&path=$path"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
