{
  "version": 2,
  "builds": [
    {
      "src": "api/addon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(realdebrid|alldebrid|torbox)=:token/(.*)",
      "dest": "/api/addon.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "cleanUrls": true
}
