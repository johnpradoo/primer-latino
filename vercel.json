{
  "rewrites": [
    {
      "source": "/realdebrid=:token/(.*)",
      "destination": "/api/addon.js?token=:token&path=$1"
    },
    {
      "source": "/alldebrid=:token/(.*)",
      "destination": "/api/addon.js?token=:token&path=$1"
    },
    {
      "source": "/torbox=:token/(.*)",
      "destination": "/api/addon.js?token=:token&path=$1"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/addon.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "cleanUrls": true
}
