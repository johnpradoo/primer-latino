{
  "rewrites": [
    {
      "source": "/(realdebrid|alldebrid|torbox)=:token/(.*)",
      "destination": "/api/addon.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/:path",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
