{
  "version": 2,
  "builds": [
    {
      "src": "api/addon.js",
      "use": "@vercel/node",
      "src": "public/index.html", "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(realdebrid|alldebrid|torbox)=[^/]+/(.*)",
      "dest": "/api/addon.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
