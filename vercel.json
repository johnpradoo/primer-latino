{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/addon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(realdebrid|alldebrid|torbox)=(?<token>[^/]+)/(?<path>.*)",
      "dest": "/api/addon.js?token=$token&path=$path"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "cleanUrls": true
}
