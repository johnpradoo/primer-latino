{
  "version": 2,
  "builds": [
    {
      "src": "api/addon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(realdebrid|alldebrid|torbox)=[^/]+/(.*)",
      "dest": "/api/addon.js"
    },
    {
      "src": "public/index.html",
      "dest": "/public/index.html"
    }
  ]
}
